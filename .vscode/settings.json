{
    "editor.rulers" : [120],
    "editor.codeActionsOnSave": {"source.fixAll.pylint": true, "source.organizeImports": true},
    "files.trimTrailingWhitespace": true,
    "python.envFile": "${workspaceFolder}/.env",
    "python.linting.pylintEnabled": true,
    "python.linting.flake8Enabled": true,
    "python.analysis.extraPaths": [
        "${workspaceFolder}/CustomLibraries",
        "${workspaceFolder}/Resources"
    ],
    "[python]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false
    },
    "robot.language-server.python": "~/Library/Caches/pypoetry/virtualenvs/you-got-robot-in-python-0Bn8XN-h-py3.10/bin/python",
    "robot.lint.robocop.enabled": true,
    "robot.pythonpath": [
        "${workspaceFolder}/CustomLibraries",
    ]
}